[default]
artifacts-rsync-host = {{ artifacts_hostname }}/{{ artifacts_path }}
# NOTE: required, but unused for daemon mode
artifacts-root = {{ artifacts_root }}
source-destination-map = ${config_root}/source-destination-map.yaml
archive-mode = daemon
rsync-options = --links,--times,--verbose,--compress
enable-parallel-archiving = yes
rsync-timeout = 600
