---
summary: Test guest-setup on Fedora Rawhide

provision:
  how: virtual
  image: Fedora-Rawhide

prepare:
  how: ansible
  playbook: ranch/public/citool-config/guest-setup/pre-artifact-installation/entrypoint.yml
  extra-args: "-e IMAGE_NAME=Fedora-Rawhide"

discover+:
  how: shell
  tests+:
    - name: Test installed koji package
      test: rpm -qi koji
