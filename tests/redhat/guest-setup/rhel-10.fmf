---
summary: Test guest-setup on RHEL 9

provision:
  how: minute
  image: RHEL-10

prepare:
  - name: pre-artifact-installation
    how: ansible
    playbook: ranch/redhat/citool-config/guest-setup/pre-artifact-installation/entrypoint.yaml
    extra-args: "-e IMAGE_NAME=RHEL-10"

discover+:
  how: shell
  tests+:
    - name: Test installed brewkoji package
      test: rpm -qi brewkoji
